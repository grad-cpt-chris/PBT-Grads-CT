{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-grad-ct"
		},
		"AzureDataLakeSource_Synapse_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeSource_Synapse'"
		},
		"AzureDataLakeStorage_target_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage_target'"
		},
		"AzureSqlDatabasesource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabasesource'"
		},
		"DataSource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DataSource'"
		},
		"LS_AdventureWorks_Source_Database_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_AdventureWorks_Source_Database'"
		},
		"LS_AdventureWorks_Target_Datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_AdventureWorks_Target_Datalake'"
		},
		"AzureDataLakeSource_Synapse_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlg2gradct.dfs.core.windows.net"
		},
		"AzureDataLakeStorage_target_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlg2gradct.dfs.core.windows.net"
		},
		"LS_AdventureWorks_Target_Datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adlg2gradct.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-grad-ct_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeSource_Synapse_accountKey": {
						"value": "[parameters('AzureDataLakeSource_Synapse_accountKey')]"
					},
					"AzureDataLakeStorage_target_accountKey": {
						"value": "[parameters('AzureDataLakeStorage_target_accountKey')]"
					},
					"AzureSqlDatabasesource_connectionString": {
						"value": "[parameters('AzureSqlDatabasesource_connectionString')]"
					},
					"DataSource_connectionString": {
						"value": "[parameters('DataSource_connectionString')]"
					},
					"LS_AdventureWorks_Source_Database_connectionString": {
						"value": "[parameters('LS_AdventureWorks_Source_Database_connectionString')]"
					},
					"LS_AdventureWorks_Target_Datalake_accountKey": {
						"value": "[parameters('LS_AdventureWorks_Target_Datalake_accountKey')]"
					},
					"AzureDataLakeSource_Synapse_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeSource_Synapse_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage_target_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage_target_properties_typeProperties_url')]"
					},
					"LS_AdventureWorks_Target_Datalake_properties_typeProperties_url": {
						"value": "[parameters('LS_AdventureWorks_Target_Datalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}