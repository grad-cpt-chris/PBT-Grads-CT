{
	"name": "Copypipeline_001",
	"properties": {
		"activities": [
			{
				"name": "ForEach_cml",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_cml",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "SalesLT.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "sql-azure-db-extract//@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"queryTimeout": {
										"value": "@item().copySource.queryTimeout",
										"type": "Expression"
									},
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_cml",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_cml",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "Address"
						},
						"destination": {
							"fileName": "SalesLTAddress"
						},
						"copySource": {
							"queryTimeout": "00:10:00"
						}
					},
					{
						"source": {
							"table": "Customer"
						},
						"destination": {
							"fileName": "SalesLTCustomer"
						},
						"copySource": {
							"queryTimeout": null
						}
					},
					{
						"source": {
							"table": "CustomerAddress"
						},
						"destination": {
							"fileName": "SalesLTCustomerAddress"
						},
						"copySource": {
							"queryTimeout": null
						}
					},
					{
						"source": {
							"table": "Product"
						},
						"destination": {
							"fileName": "SalesLTProduct"
						},
						"copySource": {
							"queryTimeout": null
						}
					},
					{
						"source": {
							"table": "ProductCategory"
						},
						"destination": {
							"fileName": "SalesLTProductCategory"
						},
						"copySource": {
							"queryTimeout": null
						}
					},
					{
						"source": {
							"table": "ProductDescription"
						},
						"destination": {
							"fileName": "SalesLTProductDescription"
						},
						"copySource": {
							"queryTimeout": null
						}
					},
					{
						"source": {
							"table": "ProductModel"
						},
						"destination": {
							"fileName": "SalesLTProductModel"
						},
						"copySource": {
							"queryTimeout": null
						}
					},
					{
						"source": {
							"table": "ProductModelProductDescription"
						},
						"destination": {
							"fileName": "SalesLTProductModelProductDescription"
						},
						"copySource": {
							"queryTimeout": null
						}
					},
					{
						"source": {
							"table": "SalesOrderDetail"
						},
						"destination": {
							"fileName": "SalesLTSalesOrderDetail"
						},
						"copySource": {
							"queryTimeout": null
						}
					},
					{
						"source": {
							"table": "SalesOrderHeader"
						},
						"destination": {
							"fileName": "SalesLTSalesOrderHeader"
						},
						"copySource": {
							"queryTimeout": null
						}
					}
				]
			}
		},
		"annotations": []
	}
}